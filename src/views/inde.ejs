




        

<div class="content-area">
    <div class="main-slider">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
          <!-- Indicators -->
          <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
        </ol>
  
        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
  
            <div class="item active">
                <img src="images/slider/sliderr5_.jpg" alt="Chania">
                <div class="carousel-caption">
                    <div class="slide-header-text wow slideInLeft" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Librería, papelería y artículos varios</div> <br />
                    <a href="products.html" class="btn btn-red slider-link wow lightSpeedIn" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Ver productos</a>
                </div>
            </div>
  
            <div class="item">
              <img src="images/slider/sliderr1.jpg" alt="Chania">
              <div class="carousel-caption">
                <div class="slide-header-text wow rotateIn" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Busque por el mejor precio</div> <br />
                <a href="products.html" class="btn btn-red slider-link wow lightSpeedIn" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Ilimitadas ofertas</a>
            </div>
        </div>
  
        <div class="item">
          <img src="images/slider/sliderr3.jpg" alt="Flower">
          <div class="carousel-caption">
           <div class="slide-header-text wow rollIn" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Un lugar de productos electrónicos</div> <br />
           <a href="products.html" class="btn btn-red slider-link wow zoomIn" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Vea todos los artículos</a>
       </div>
   </div>
  
   <div class="item">
      <img src="images/slider/sliderr4.jpg" alt="Flower">
      <div class="carousel-caption">
          <div class="slide-header-text wow bounceInLeft" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Las mejores mochilas escolares y para el trabajo</div> <br />
          <a href="products.html" class="btn btn-red slider-link wow slideInRight" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Eche un vistazo</a>
      </div>
  </div>
  </div>
  
  <!-- Left and right controls -->
  <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  
  </a>
  
  <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
  
  </div>
  </div> <!-- End Main slider class -->
  
  
  <div class="featured-products">
    <div class="container">
        <h2 class="title-div wow slideInLeft pull-left" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Productos destacados</h2><div class="clear"></div>
        <div class="featured-navigation pull-right">
            <span class="">
                <a class="owl-prev owl-navigaiton"><i class="fa fa-angle-double-right"></i></a>
            </span>
            <span class="stop">
                <a class="owl-next owl-navigaiton">||</a>
            </span>
            <span class="">
                <a class="owl-next owl-navigaiton"><i class="fa fa-angle-double-left"></i></a>
            </span>
            
        </div>
        <div class="clear"></div>
        <div class="featured-items">
            <!-- Set up your HTML -->
            <div class="owl-carousel">
                <% ofertauno.forEach(function (ofertaun) { %>
                <div class="item featured2">
                    <div class="item-full animated featured2-inner  width0">
                      <a href="/ofertaunoredirect/<%= ofertaun.id %>"><h4><%= ofertaun.title %></h4></a>
                      <span class="prev-price">
                              <del>$ <%= ofertaun.oldprice %></del>
                          </span>
                        <h5><%= ofertaun.price %></h5>
                        <a href="/addtocardofertauno/<%= ofertaun.id %>" class="btn btn-cart">
                            Al carro
                        </a>                  
                    </div>
                    
                        <img src="<%= ofertaun.image %>" class="img img-responsive" alt="Product1">
                    
                </div> <!-- Single Featured Item -->      
                <% }) %>
                
            </div>
        </div>
    </div>
  </div> <!--End Featured products Div-->
  
  
  <div class="latest-products">
    <div class="container">
        <h2 class="title-div wow slideInLeft" data-wow-duration="1s" data-wow-delay="0s" data-wow-offset="10">Nuestras ofertas</h2>
        <div class="products">
            <div class="row">
  
                <% ofertados.forEach(function (ofertado) { %>
                <div class="col-md-3">
                    <div class="product-item">
                        <div class="product-borde-inner">
                            <a href="/ofertadosredirect/<%= ofertado.id %>">
                                <img src="<%= ofertado.image %>" class="img img-responsive"/>
                            </a> 
                            <div class="product-price">
                                <a href="product_single.html"><%= ofertado.title %></a><br />
                                <span class="prev-price">
                                    <del>$<%= ofertado.oldprice %></del>
                                </span>
                                <span class="current-price">
                                      $<%= ofertado.price %> 
                                </span>
                            </div>
  
                            <a href="/ofertadosredirect/<%= ofertado.id %>"  class="btn btn-cart text-center add-to-cart pull-right">
                                <i class="fa fa-cart-plus"></i>
                                Agregar al carro
                            </a>
                            <div class="clearfix"></div>
                        </div>
                    </div> 
                </div><!-- End Latest products[single] -->  
                <% }) %>
  
                <div class="clearfix"></div>
  
            </div> <!-- End Latest products row-->
           
            <div class="clear"></div>
        </div> <!-- End products div-->
    </div> <!-- End container latest products-->
  </div>  <!-- End Latest products -->
  
  
  
  
  
  
  
